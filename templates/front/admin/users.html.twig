{% extends 'front/admin/base.html.twig' %}
    {% block customstylesheets %}
        <link rel="stylesheet" href="{{ asset('build/app.css') }}">
        <link rel="stylesheet" href="{{ asset('build/styles/dashboard.css') }}">
    {% endblock %}
{% block mainadmin %}

    <h2>Users</h2>

    <div class="table-responsive">
        <table class="table table-striped table-sm">
            <thead>
            <tr>
                <th>#</th>
                <th>Name</th>
                <th>Last name</th>
                <th>Email</th>
                <th>Delete</th>
            </tr>
            </thead>
            <tbody>
            {% for i in 1..10 %}
            <tr>
                <td>{{ i }}</td>
                <td>Adam</td>
                <td>Smith</td>
                <td>adam@smith.com</td>
                <td><a href="#" onclick="return confirm('Are you sure?');"><i class="fas fa-trash"></i></a></td>
            </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>

{% endblock %}